{
   "background": {
      "persistent": false,
      "scripts": [ "background.js" ]
   },
   "browser_action": {

   },
   "content_scripts": [ {
      "css": [ "data/styles/common.css" ],
      "exclude_globs": [ "*://www.google.*/recaptcha/*", "*://www.google.*/maps*", "*://contacts.google.*/*" ],
      "include_globs": [ "*://www.google.*/", "*://www.google.*/*", "*://scholar.google.*/*", "*://images.google.*/*", "*://news.google.*/*", "*://encrypted.google.*/*", "*://accounts.google.*/*", "*://myaccount.google.*/*", "*://translate.google.*/*" ],
      "js": [ "data/styles/common.js" ],
      "matches": [ "*://*/*" ],
      "run_at": "document_start"
   }, {
      "css": [ "data/styles/accounts.css" ],
      "include_globs": [ "*://accounts.google.*/*", "*://myaccount.google.*/*" ],
      "matches": [ "*://*/*" ],
      "run_at": "document_start"
   }, {
      "css": [ "data/styles/travel.css" ],
      "include_globs": [ "*://www.google.*/travel/*", "*://www.google.*/flights?*" ],
      "matches": [ "*://*/*" ],
      "run_at": "document_start"
   }, {
      "css": [ "data/styles/search.css" ],
      "include_globs": [ "*://www.google.*/search?*", "*://www.google.*/webhp?*" ],
      "matches": [ "*://*/*" ],
      "run_at": "document_start"
   }, {
      "css": [ "data/styles/scholar.css" ],
      "include_globs": [ "*://scholar.google.*/*" ],
      "matches": [ "*://*/*" ],
      "run_at": "document_start"
   }, {
      "css": [ "data/styles/translate.css" ],
      "include_globs": [ "*://translate.google.*/*" ],
      "matches": [ "*://*/*" ],
      "run_at": "document_start"
   }, {
      "all_frames": true,
      "css": [ "data/styles/common.css", "data/styles/widget.css" ],
      "include_globs": [ "*://*.google.*/widget/app/*", "*://*.google.com/u/0/widget/app*" ],
      "js": [ "data/styles/common.js" ],
      "matches": [ "*://*/*" ],
      "run_at": "document_start"
   }, {
      "all_frames": true,
      "css": [ "data/styles/preferences.css" ],
      "include_globs": [ "*://*.google.*/preferences*" ],
      "matches": [ "*://*/*" ],
      "run_at": "document_start"
   } ],
   "default_locale": "en",
   "description": "__MSG_app_description__",
   "homepage_url": "https://add0n.com/dark-theme.html?from=google",
   "icons": {
      "128": "data/icons/128.png",
      "16": "data/icons/16.png",
      "19": "data/icons/19.png",
      "256": "data/icons/256.png",
      "32": "data/icons/32.png",
      "38": "data/icons/38.png",
      "64": "data/icons/64.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Dark Theme for Googleâ„¢",
   "options_ui": {
      "chrome_style": true,
      "page": "data/options/index.html"
   },
   "permissions": [ "storage", "alarms", "idle" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.3.1"
}
